{
    "name": "@savannstm/marshal",
    "version": "0.4.0",
    "author": "hyrious <hyrious@outlook.com>",
    "contributors": [
        "savannstm <savannstm@gmail.com>"
    ],
    "description": "Ruby marshal, that can be safely used both in browser and Node.js.",
    "license": "MIT",
    "repository": "savannstm/marshal",
    "keywords": [
        "ruby",
        "marshal",
        "serialize"
    ],
    "exports": {
        "node": {
            "import": "./dist/marshal.mjs",
            "require": "./dist/marshal.js"
        },
        "default": "./dist/marshal.mjs"
    },
    "type": "module",
    "module": "index.ts",
    "main": "dist/bundle.js",
    "types": "dist/bundle.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": "^14.18.0 || >=16.0.0"
    },
    "devDependencies": {
        "@eslint/js": "^10.0.0",
        "@hyrious/marshal": "^0.3.3",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^11.1.6",
        "@types/bun": "^1.1.6",
        "eslint": "^9.8.0",
        "rollup": "^4.19.1",
        "rollup-plugin-dts": "^6.1.1",
        "typescript-eslint": "^7.18.0",
        "uvu": "^0.5.6"
    },
    "peerDependencies": {
        "typescript": "^5.5.2"
    },
    "scripts": {
        "build": "rollup -c",
        "test": "node-ts ./tests/index.ts"
    }
}
